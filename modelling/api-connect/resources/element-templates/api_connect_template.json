{
  "$schema": "https://unpkg.com/@camunda/element-templates-json-schema/resources/schema.json",
  "name": "API Connect",
  "id": "onl.mrtn.camunda.http",
  "description": "Connect to a REST API.",
  "entriesVisible": false,
  "icon": {
    "contents": "data:image/svg+xml;base64,"
  },
  "category": {
    "id": "connectors",
    "name": "Connectors"
  },
  "appliesTo": [
    "bpmn:Task"
  ],
  "elementType": {
    "value": "bpmn:ServiceTask"
  },
  "groups": [
    {
      "id": "endpoint",
      "label": "Endpoint"
    },
    {
      "id": "authentication",
      "label": "Authentication"
    },
    {
      "id": "mapping",
      "label": "Mapping"
    }
  ],
  "properties": [
    {
      "type": "Hidden",
      "value": "${api_connect}",
      "binding": {
        "type": "property",
        "name": "camunda:delegateExpression"
      }
    },
    {
      "label": "URL",
      "group": "endpoint",
      "type": "String",
      "binding": {
        "type": "camunda:field",
        "name": "URL"
      },
      "constraints": {
        "notEmpty": true,
        "pattern": {
          "value": "^(=|http://|https://|secrets).*$",
          "message": "Must be a http(s) URL."
        }
      }
    },
    {
      "label": "Authorization Header",
      "description": "Here it would be possible to define an authorisation header string if necessary, for example to specify a bearer token. Otherwise indicate “None”.",
      "group": "authentication",
      "type": "String",
      "value": "None",
      "binding": {
        "type": "camunda:field",
        "name": "authorization"
      },
      "constraints": {
        "notEmpty": true
      },
      "optional": true
    },
    {
      "label": "Automatic Incoming Variables",
      "description": "Shall workflow variables are automatically be created based on the response JSON object?",
      "group": "mapping",
      "type": "Dropdown",
      "value": "true",
      "binding": {
        "type": "camunda:field",
        "name": "automatic_variables"
      },
      "choices": [
        { "name": "Yes", "value": "true" },
        { "name": "No", "value": "false" }
      ]
    },
    {
      "label": "Result Variable",
      "description": "A global variable can be defined here to extract data from the JSON object later. Otherwise indicate “None”.",
      "group": "mapping",
      "type": "String",
      "value": "None",
      "binding": {
        "type": "camunda:field",
        "name": "result_variable"
      },
      "constraints": {
        "notEmpty": true
      },
      "optional": true
    },
    {
      "label": "Outgoing Variables",
      "description": "By default, all workflow variables are transferred to the endpoint. If only selected workflow variables are to be transferred, they must be listed using “Assignment type” as “List” with the variable name.",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "api_variables"
      },
      "optional": true
    },
    {
      "label": "Incoming Variables",
      "description": "By default, workflow variables are automatically created based on the response JSON object. If only selected workflow variables should be created, they must be listed using “Assignment type” as “List” with the variable name.",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "result_variables"
      },
      "optional": true
    }
  ]
}