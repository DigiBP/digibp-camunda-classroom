{
  "$schema": "https://unpkg.com/@camunda/element-templates-json-schema/resources/schema.json",
  "name": "API Connect",
  "id": "onl.mrtn.camunda.http",
  "description": "Connect to a REST API.",
  "entriesVisible": false,
  "icon": {
    "contents": "data:image/svg+xml;base64,"
  },
  "category": {
    "id": "connectors",
    "name": "Connectors"
  },
  "appliesTo": [
    "bpmn:Task"
  ],
  "elementType": {
    "value": "bpmn:ServiceTask"
  },
  "groups": [
    {
      "id": "endpoint",
      "label": "Endpoint"
    },
    {
      "id": "authentication",
      "label": "Authentication"
    },
    {
      "id": "mapping",
      "label": "Mapping"
    }
  ],
  "properties": [
    {
      "type": "Hidden",
      "value": "${api_connect}",
      "binding": {
        "type": "property",
        "name": "camunda:delegateExpression"
      }
    },
    {
      "label": "üîó URL",
      "group": "endpoint",
      "type": "String",
      "binding": {
        "type": "camunda:field",
        "name": "URL"
      },
      "constraints": {
        "notEmpty": true,
        "pattern": {
          "value": "^(=|http://|https://|secrets).*$",
          "message": "Must be a http(s) URL."
        }
      }
    },
    {
      "label": "üîë Authorization Header",
      "description": "Here it would be possible to define an authorisation header string if necessary, for example to specify a bearer token. Otherwise, indicate ‚ÄúNone‚Äù.",
      "group": "authentication",
      "type": "String",
      "value": "None",
      "binding": {
        "type": "camunda:field",
        "name": "authorization"
      },
      "constraints": {
        "notEmpty": true
      },
      "optional": true
    },
    {
      "label": "ü™Ñ Automatic ‚û°Ô∏è Outgoing Variables",
      "description": "Shall workflow variables automatically be transferred to the endpoint?",
      "group": "mapping",
      "type": "Dropdown",
      "value": "true",
      "binding": {
        "type": "camunda:field",
        "name": "automatic_api_variables"
      },
      "choices": [
        { "name": "Yes", "value": "true" },
        { "name": "No", "value": "false" }
      ]
    },
    {
      "label": "ü™Ñ Automatic ‚¨ÖÔ∏è Incoming Variables",
      "description": "Shall workflow variables automatically be created based on the response JSON object?",
      "group": "mapping",
      "type": "Dropdown",
      "value": "true",
      "binding": {
        "type": "camunda:field",
        "name": "automatic_result_variables"
      },
      "choices": [
        { "name": "Yes", "value": "true" },
        { "name": "No", "value": "false" }
      ]
    },
    {
      "label": "üü∞ Result Variable",
      "description": "A global variable can be defined here to extract data from the JSON object later. Otherwise, indicate ‚ÄúNone‚Äù.",
      "group": "mapping",
      "type": "String",
      "value": "None",
      "binding": {
        "type": "camunda:field",
        "name": "result_variable"
      },
      "constraints": {
        "notEmpty": true
      },
      "optional": true
    },
    {
      "label": "‚û°Ô∏è Outgoing Variables",
      "description": "By default, all workflow variables are transferred to the endpoint. If only selected workflow variables are to be transferred, they must be listed using ‚ÄúAssignment type‚Äù as ‚ÄúList‚Äù with the variable name.",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "api_variables"
      },
      "optional": true
    },
    {
      "label": "‚¨ÖÔ∏è Incoming Variables",
      "description": "By default, workflow variables are automatically created based on the response JSON object. If only selected workflow variables should be created, they must be listed using ‚ÄúAssignment type‚Äù as ‚ÄúList‚Äù with the variable name.",
      "binding": {
        "type": "camunda:inputParameter",
        "name": "result_variables"
      },
      "optional": true
    }
  ]
}